{
  "expo": {
    "name": "lovenote",
    "slug": "lovenote",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/lovenote-icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/lovenote-icon.png",
        "backgroundColor": "#6D2E4B"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/lovenote-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#6D2E4B"
        }
      ],
      [
        "expo-font",
        {
          "fonts": ["./assets/fonts/SpaceMono-Regular.ttf"]
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "d797949d-aa32-449a-b065-f3005666265d"
      }
    },
    "owner": "tmmosher"
  }
}
